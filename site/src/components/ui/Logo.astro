---
interface Props {
  className?: string;
}

const { className } = Astro.props;
---

<a href="/" class:list={['block focus-visible:outline-none', className]} aria-label="Joe the Painter homepage">
  <span class="font-heading font-bold text-2xl text-text leading-none transition-colors duration-fast ease-default">
    JOE <span class="text-primary">THE PAINTER</span>
  </span>
</a>